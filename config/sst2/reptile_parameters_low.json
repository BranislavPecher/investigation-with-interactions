{
    "output_folder": "results/stability/investigation/sst2/parameter_optimisation/Reptile_low",
    "use_timestamp_dir": false,
    "preprocessing": {
        "serialized_folder": "results/sst2/data",
        "load_test_data": false
    },
    "modeling": {
        "meta_learning": true,
        "parameter_optimisation": true,
        "train_test_split": 0.2,
        "train_valid_split": 0.2,
        "data_split_count": 1,
        "labels_to_use": 1000,
        "run_count": 1,
        "parameters_to_search": {
            "num_tasks_per_batch": [6, 8, 10],
