        "feature": "sentence_embedded",
        "base_models": [
            {
                "model": "MetaSimpleCnnText",
                "params": {
                    "n_filters": 128,
                    "filter_sizes": 5,
                    "pool_size": 2,
                    "hidden_size": 128,
                    "loss_function": "cross_entropy"
                }
            }
        ],
        "meta_models": [
            {
                "model": "Reptile",
                "params": {
                    "meta_optimizer": "SGD",
                    "base_optimizer": "Adam"
                }
            }
        ],
        "serialize_best_models": true,
        "use_gpu": true
    },
    "evaluation": {}
