{
    "output_folder": "results/sst2",
    "data": {
        "source": "datasets",
        "path": "glue",
        "split": "sst2",
        "keys": ["train", "validation"],
        "data_fields": ["sentence"],
        "label_field": "label"
    },
    "train_test_split": null,
    "preprocessing": {
        "transformers": [
            {
                "transformer": "ColumnsSelector",
