                }
            },
            {
                "transformer": "NaNsFilter",
                "params": {}
            },
            {
                "transformer": "TextCleaner",
                "params": {
                    "columns": ["sentence"]
                }
            },
            {
                "transformer": "LowercaseTransformer",
                "params": {
                    "columns": ["sentence"]
                }
            },
            {
                "transformer": "BertTransformer",
                "params": {
                    "columns": ["sentence"],
                    "size": 50
                }
            }
