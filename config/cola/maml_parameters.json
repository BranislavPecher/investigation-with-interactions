{
    "output_folder": "results/stability/investigation/cola/parameter_optimisation/MAML",
    "use_timestamp_dir": false,
    "preprocessing": {
        "serialized_folder": "results/cola/data",
        "load_test_data": false
    },
    "modeling": {
        "meta_learning": true,
        "parameter_optimisation": true,
        "train_test_split": 0.2,
        "train_valid_split": 0.2,
        "labels_to_use": 1000,
        "mitigations_strategies": {
            "data_split": 1,
            "label_selection": 1,
            "repeated_runs": 1,
            "repeated_evaluation": 1
        },
        "parameters_to_search": {
            "num_tasks_per_batch": [4, 8, 16, 32],
            "num_batches": [8, 16, 32, 64],
            "num_epochs": [1, 3, 5, 10],
            "base_lr": [0.001, 0.01, 0.1],
            "meta_lr": [0.001, 0.01, 0.1],
            "meta_optimizer": ["Adam"],
            "base_optimizer": ["SGD"],
            "inner_iterations": [1]
        },
        "meta_learning_parameters": {
            "evaluation_batch": 128
        },
        "dataset_parameters": {
            "num_shots": 5,
            "num_shots_test": 15,
            "max_sentence_length": 50,
            "embedding_dimension": 768,
            "num_classes": 2
        },
        "feature": "sentence_embedded",
        "base_models": [
            {
                "model": "MetaSimpleCnnText",
                "params": {
                    "n_filters": 128,
                    "filter_sizes": 5,
                    "pool_size": 2,
                    "hidden_size": 128,
                    "loss_function": "cross_entropy"
                }
            }
        ],
        "meta_models": [
            {
                "model": "MAML",
                "params": {
                    "meta_optimizer": "Adam",
                    "base_optimizer": "SGD",
                    "inner_iterations": 1
                }
            }
        ],
        "serialize_best_models": true,
        "use_gpu": true
    },
    "evaluation": {}
}
